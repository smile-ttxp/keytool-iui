<?xml version="1.0" encoding="utf-8"?>
<jnlp spec="$jnlpspec" codebase="http://www.fuin.org/keytool-iui" href="$outputFile">
  <information>
#if($project.Name)
    <title>$project.Name</title>
#end
#if($project.Organization.Name)
    <vendor>$project.Organization.Name</vendor>
#end
#if($project.Url)
    <homepage href="$project.Url"/>
#end
#if($project.Description)
    <description>$project.Description</description>
#end
#if($offlineAllowed)
    <offline-allowed/>
#end
  </information>
#if($allPermissions)
  <security>
     <all-permissions/>
  </security>
#end
  <resources>
    <j2se version="1.6+" max-heap-size="512m"/>
    $dependencies
	<property name="_appli.title" value="KeyToolIUI"/> 
	<property name="_appli.version" value="${project.version}"/> 
	<property name="_appli.name.short" value="ktl"/> 
  </resources>
  <application-desc main-class="$mainClass"/>  
</jnlp>

